{
  "manifest_version": 2,

  "name": "Phish Lens",
  "version": "1.3",

  "icons": {
    "48": "lens.png"
  },

  "permissions": [
    "tabs", "<all_urls>",
    "notifications",
    "webNavigation",
    "storage",
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "lens.png",
    "default_popup": "popup.html"
  },

  "background": {
    "page" : "background.html",
    "persistent": false
  }
}
